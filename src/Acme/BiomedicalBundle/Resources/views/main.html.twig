<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="application/xhtml+xml; charset=UTF-8" accept-charset="UTF-8"/>
<title>{% block title %}{{title}}{% endblock %}</title>
<link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
{% block stylesheets %}
<link href="{{ asset('bundles/acmebiomedical/css/main.css') }}"
	media="all" rel="stylesheet" type="text/css" />
<link rel="stylesheet"
	href="http://code.jquery.com/ui/1.10.4/themes/smoothness/jquery-ui.css">
{% endblock %}  
{% block javascripts %}
<script src="http://code.jquery.com/jquery-1.9.1.js"></script>
<script src="http://code.jquery.com/ui/1.10.4/jquery-ui.js"></script>
{% endblock %}
</head>
<body>
	{% block body %}
	<div id="header">
		{% block header %}
		<div id="head">
			<ul id="menu">
				<li class="nav_text"><a href="{{path('acme_biomedical_homepage')}}">BioMedicalPortal</a></li>
				<li class="nav_text"><a href="#">{% trans %}distances.semantiques{% endtrans %}</a>
					<ul>
						<li class="nav_text_horiz"><a href="{{path('acme_biomedical_semantic_distance_two_concepts')}}">{% trans %}entre.deux.concepts{% endtrans %}</a></li>
						<li class="nav_text_horiz"><a href="{{path('acme_biomedical_semantic_distance_concept_dist')}}">{% trans %}avec.un.concept.et.une.distance{% endtrans %}</a></li>
					</ul>
				</li>
				{% block doc %}
				<li class="nav_text"><a href="api/doc/">{% trans %}documentation.api{% endtrans %}</a></li>
				{% endblock %}
			</ul>
			<a href="{{  path(app.request.get('_route'), app.request.get('_route_params')|merge({'_locale': 'fr'}))}}" >
			<img id="flag_1" alt="Drapeau français" src="{{ asset('bundles/acmebiomedical/images/drapeau-Francais.jpg') }}" title="{% trans %}drapeau.français{% endtrans %}" lang="fr" />
			</a>
			<a href="{{  path(app.request.get('_route'), app.request.get('_route_params')|merge({'_locale': 'en'}))}}">
			<img id="flag_2" alt="Drapeau anglais" src="{{ asset('bundles/acmebiomedical/images/drapeau-Anglais.jpg') }}" title="{% trans %}drapeau.anglais{% endtrans %}" lang="en" />
			</a>
		</div>
		{% endblock %}
	</div>
	<div id="content">
		{% block content %} 
		{% endblock %}
	</div>
	<div id="footer">{% block footer %} Copyright 2014 Burc Pierre,
		Duplouy Olivier{% endblock %}</div>
	{% endblock %}
</body>
</html>